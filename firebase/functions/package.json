{
    "name": "functions",
    "description": "Cloud Functions for Firebase",
    "scripts": {
        "lint": "tslint --project tsconfig.json",
        "setup": "firebase use --add",
        "build": "parcel build src/index.ts --target node --detailed-report",
        "debug": "firebase emulators:start --only functions --inspect-functions",
        "watch": "parcel src/index.ts --target node",
        "serve": "firebase emulators:start --only functions --inspect-functions",
        "predeploy": "npm run build",
        "deploy": "firebase deploy --only functions"
    },
    "engines": {
        "node": "10"
    },
    "main": "dist/index.js",
    "dependencies": {
        "@google-cloud/logging-bunyan": "^3.0.2",
        "@types/bunyan": "^1.8.6",
        "@types/child-process-promise": "^2.2.1",
        "@types/passport": "^1.0.3",
        "@types/uuid": "^8.3.0",
        "bunyan": "^1.8.15",
        "child-process-promise": "^2.2.0",
        "cookie-session": "^1.4.0",
        "cors": "^2.8.4",
        "express": "^4.17.1",
        "express-jwt": "^5.3.1",
        "firebase-admin": "^8.6.0",
        "firebase-functions": "3.6.0",
        "firebase-tools": "8.12.0",
        "gaxios": "^4.0.1",
        "grpc": "^1.24.3",
        "i18n-iso-countries": "^6.0.0",
        "jwks-rsa": "^1.3.0",
        "lodash": "^4.17.19",
        "passport": "^0.4.1",
        "passport-auth0": "^1.3.2",
        "uuid": "^7.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-flow": "^7.9.0",
        "@firebase/app-types": "^0.3.9",
        "@types/cors": "^2.8.9",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.20.0",
        "eslint": "^5.16.0",
        "eslint-plugin-promise": "^4.0.1",
        "firebase-functions-test": "^0.1.6",
        "parcel-bundler": "^1.12.3",
        "typescript": "^3.9.9"
    },
    "private": true
}
