{
	"name": "functions",
	"description": "Cloud Functions for Firebase",
	"scripts": {
		"lint": "tslint --project tsconfig.json",
		"setup": "firebase use --add",
		"build": "parcel build --target default src/index.ts --detailed-report",
		"debug": "firebase emulators:start --only functions --inspect-functions",
		"watch": "parcel watch --target default --no-hmr src/index.ts",
		"serve": "firebase emulators:start --only functions --inspect-functions",
		"predeploy": "npm run build",
		"deploy": "firebase deploy --only functions",
		"test": "firebase emulators:exec --only firestore \"npx nyc --reporter=lcov --reporter=text-summary ava --timeout=1m\""
	},
	"targets": {
		"default": {
			"context": "node",
			"engines": {
				"node": "12"
			}
		}
	},
	"engines": {
		"node": "12"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		],
		"files": [
			"test/*.test.ts"
		]
	},
	"prettier": {
		"trailingComma": "es5",
		"tabWidth": 4,
		"semi": false,
		"singleQuote": true
	},
	"main": "dist/index.js",
	"dependencies": {
		"@babel/plugin-transform-runtime": "^7.13.15",
		"@babel/preset-env": "^7.14.5",
		"@babel/preset-typescript": "^7.13.0",
		"@firebase/app": "^0.6.21",
		"@google-cloud/logging-bunyan": "^3.0.2",
		"@google-cloud/pubsub": "^2.16.1",
		"@google-cloud/storage": "^5.8.5",
		"@google-cloud/trace-agent": "^5.1.6",
		"@parcel/transformer-typescript-tsc": "^2.0.0-alpha.3",
		"@types/child-process-promise": "^2.2.1",
		"@types/express-serve-static-core": "^4.17.19",
		"@types/luxon": "^1.27.1",
		"@types/node-fetch": "^2.5.11",
		"@types/passport": "^1.0.7",
		"@types/uuid": "^8.3.0",
		"bunyan": "^1.8.15",
		"child-process-promise": "^2.2.0",
		"cookie-session": "^1.4.0",
		"cors": "^2.8.4",
		"express": "^4.18.2",
		"express-jwt": "^6.0.0",
		"firebase-admin": "^10.0.1",
		"firebase-functions": "^3.18.0",
		"firebase-tools": "^9.23.3",
		"gaxios": "^4.2.1",
		"grpc": "^1.24.9",
		"i18n-iso-countries": "^6.7.0",
		"jwks-rsa": "^1.12.3",
		"lodash": "^4.17.21",
		"luxon": "^2.0.1",
		"node-cache": "^5.1.2",
		"node-fetch": "^2.6.7",
		"parcel": "^2.0.0-nightly.720",
		"passport": "^0.4.1",
		"passport-auth0": "^1.4.1",
		"uuid": "^7.0.3"
	},
	"devDependencies": {
		"@babel/core": "^7.12.0",
		"@babel/preset-flow": "^7.13.13",
		"@firebase/app-types": "^0.3.9",
		"@types/bunyan": "^1.8.6",
		"@types/cookie-session": "^2.0.42",
		"@types/cors": "^2.8.10",
		"@types/express": "4.17.11",
		"@types/passport-auth0": "^1.0.4",
		"@typescript-eslint/eslint-plugin": "^4.22.1",
		"@typescript-eslint/parser": "^4.22.1",
		"ava": "^3.15.0",
		"eslint": "7",
		"eslint-plugin-promise": "^4.3.1",
		"firebase-functions-test": "^0.1.6",
		"node-mocks-http": "^1.10.1",
		"nyc": "^15.1.0",
		"prettier": "2.2.1",
		"ts-node": "^10.0.0",
		"typescript": "^3.9.10"
	},
	"private": true
}
